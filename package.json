{
  "name": "podverse",
  "version": "2.6.9",
  "private": true,
  "contributors": [
    "Creon Creonopoulos",
    "Mitch Downey",
    "Gary Johnson"
  ],
  "scripts": {
    "dev": "react-native start",
    "dev:android": "react-native run-android",
    "dev:ios": "react-native run-ios --simulator 'iPhone 11 Pro Max'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write src/**/*.ts*",
    "test:e2e:android": "export $(cat .env | xargs) && DEVICE_TYPE=Android BROWSERSTACK_DEVICE='Google Pixel 3' BROWSERSTACK_OS_VERSION='9.0' node ./functions/tests/e2e/index.js",
    "test:e2e:ios": "export $(cat .env | xargs) && DEVICE_TYPE=iOS BROWSERSTACK_DEVICE='iPhone 11 Pro Max' BROWSERSTACK_OS_VERSION='13.0' node ./functions/tests/e2e/index.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.6.2",
    "@react-native-community/masked-view": "0.1.9",
    "@react-native-community/netinfo": "4.4.0",
    "@types/react-native-background-timer": "2.0.0",
    "@types/react-native-rss-parser": "1.4.0",
    "@types/validator": "12.0.1",
    "appcenter": "3.0.2",
    "appcenter-analytics": "3.0.2",
    "appcenter-crashes": "3.0.2",
    "axios": "0.19.0",
    "bad-words": "3.0.3",
    "bottleneck": "2.19.1",
    "eventemitter3": "4.0.0",
    "he": "1.2.0",
    "linkifyjs": "2.1.8",
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-background-downloader": "2.3.4",
    "react-native-background-fetch": "2.7.1",
    "react-native-background-timer": "2.1.1",
    "react-native-camera": "^3.30.0",
    "react-native-cheerio": "1.0.0-rc.4",
    "react-native-config": "1.0.0",
    "react-native-device-info": "5.5.3",
    "react-native-dialog": "5.6.0",
